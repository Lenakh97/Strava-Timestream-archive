  },
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-ses": "3.554.0",
    "@aws-sdk/client-ssm": "3.474.0",
    "@aws-sdk/client-timestream-query": "3.474.0",
    "@aws-sdk/client-timestream-write": "3.474.0",
    "@aws-sdk/util-dynamodb": "3.474.0",
    "@bifravst/eslint-config-typescript": "6.0.23",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "13.1.5",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "17.0.0",
    "@swc/cli": "0.1.63",
    "@swc/jest": "0.2.29",
    "@types/aws-lambda": "^8.10.130",
    "@types/jest": "29.5.11",
    "@types/yazl": "2.4.5",
    "aws-cdk": "2.115.0",
    "aws-cdk-lib": "2.115.0",
    "cdk": "2.115.0",
    "glob": "10.3.10",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "tsx": "4.7.2",
    "typescript": "5.3.3",
    "yazl": "2.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": "@bifravst/prettier-config",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.474.0",
    "@bifravst/prettier-config": "^1.0.0",
    "@nordicsemiconductor/from-env": "3.0.0",
    "@nordicsemiconductor/timestream-helpers": "6.0.1",
    "date-fns": "3.6.0"
  }
