  },
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@aws-sdk/client-ssm": "3.473.0",
    "@aws-sdk/client-timestream-query": "3.473.0",
    "@aws-sdk/client-timestream-write": "3.473.0",
    "@aws-sdk/util-dynamodb": "3.473.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.3",
    "@nordicsemiconductor/eslint-config-asset-tracker-cloud-typescript": "17.0.0",
    "@swc/cli": "0.1.62",
    "@swc/jest": "0.2.24",
    "@types/aws-lambda": "^8.10.114",
    "@types/jest": "29.5.11",
    "@types/yazl": "2.4.2",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "aws-cdk": "2.114.1",
    "aws-cdk-lib": "2.114.1",
    "cdk": "2.114.1",
    "glob": "9.3.2",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "prettier": "2.8.4",
    "tsx": "3.14.0",
    "typescript": "5.3.3",
    "yazl": "2.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest"
      ]
    },
    "testRegex": ".+\\.spec\\.ts$",
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "lint-staged": {
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.473.0",
    "@nordicsemiconductor/from-env": "3.0.0",
    "@nordicsemiconductor/timestream-helpers": "6.0.1",
